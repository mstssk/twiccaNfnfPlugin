<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mstssk.twiccaplugins.nfnf"
    android:versionCode="3"
    android:versionName="1.5.1" >

    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="10" />
   	<supports-screens android:xlargeScreens="true"
		android:largeScreens="true" android:normalScreens="true"
		android:smallScreens="true" android:anyDensity="true" />
	<uses-feature android:name="android.hardware.touchscreen" android:required="false"/>
	<uses-feature android:name="android.hardware.faketouch" android:required="false"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:debuggable="false">
        <activity
            android:label="@string/nfnf"
            android:name=".Nfnf" android:theme="@android:style/Theme.Translucent.NoTitleBar">
            <intent-filter >
                <action android:name="jp.r246.twicca.ACTION_EDIT_TWEET" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>